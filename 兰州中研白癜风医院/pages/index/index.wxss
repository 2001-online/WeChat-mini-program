/* 热门问答 */
.jiankang{
  width: 96%;
  margin: 0 auto;
  /* height: 400rpx; */
  background-color: #fff;
  border-radius: 10rpx;
  padding-bottom: 50rpx;
}
.jiankang .top{
  display: flex;
  justify-content: space-between;
}
.jiankang .top .title{
  font-size: 36rpx;
  font-weight: bold;
  margin: 20rpx 0 0 20rpx;
}
.jiankang .top .gengduo{
  font-size: 30rpx;
  color: rgba(0, 0, 0, 0.589);
  margin: 20rpx 20rpx 0 0;
}
/* 热门问答默认样式 */
.jiankang .textList{
  height: 130rpx;
  /* background-color: rgb(233, 175, 175); */
  margin-top: 30rpx;
  margin-left: 10rpx;
  padding: 0 20rpx;
  transition-property: height;
  transition-duration: 0.3s;
  transition-timing-function: ease-out;
}
/* 热门问答展开样式 */
.textListShow{
  height: 250rpx;
  /* background-color: rgb(233, 175, 175); */
  margin-top: 30rpx;
  margin-left: 10rpx;
  padding: 0 20rpx;
  transition-property: height;
  transition-duration: 0.3s;
  transition-timing-function: ease-out;
}
.textListItem .textTitle{
  /* color: rgba(60, 168, 10, 0.795); */
  margin-left: 20rpx;
  font-size: 30rpx;
}
/* 热门回答展开样式 */
.active{
  margin: 10rpx 20rpx 0 20rpx;
}
/* 热门回答默认样式 */
.textListItem .text{
  margin: 10rpx 20rpx 0 20rpx;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
}
.textListItem text{
  font-size: 24rpx;
  color: rgba(0, 0, 0, 0.418);
}
/* 展开与收起按钮 */
.contentState{
