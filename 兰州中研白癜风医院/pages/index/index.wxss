  border-bottom: 2rpx solid rgb(126, 235, 83);
  border-radius: 10rpx;
}
.doctor .doctorList .doctorListItem .doctorImg image{
  width: 160rpx;
  height: 160rpx;
  margin-top: 20rpx;
}
.doctor .doctorList .doctorListItem .doctorInfo{
  margin-left: 20rpx;
  margin-top: 20rpx;
}
.doctor .doctorList .doctorListItem .doctorInfo .name{
  display: flex;
}
.doctor .doctorList .doctorListItem .doctorInfo .name .a{
  font-size: 32rpx;
}
.doctor .doctorList .doctorListItem .doctorInfo .name .b{
  color: rgb(99, 189, 231);
  margin-left: 20rpx;
  font-size: 28rpx;flex: 1;
}
.doctor .doctorList .doctorListItem .doctorInfo .name .c{
  font-size: 28rpx;
  color: white;
  background-color: rgb(99, 202, 78);
  padding: 8rpx 20rpx;
  border-radius: 50rpx;
}
.doctor .doctorList .doctorListItem .doctorInfo .text text{
  font-size: 26rpx;
  color: rgba(0, 0, 0, 0.644);
}
