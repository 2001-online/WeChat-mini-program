
.content{
  color: wihte;
}
.content .topBanner{
  width: 100%;
  height: 220rpx;
}
.content .topBanner image{
  width: 100%;
  height: 220rpx;
}
.queren{
  width: 100%;
  height: 100%;
}
.queren .title{
  font-size: 36rpx;
  margin: 30rpx;
}
.queren .content{
  width: 100%;
}
/* 选择医生和预约时间样式 */
.queren .content .form-item1{
  height: 100rpx;
  margin-left: 30rpx;
}
.queren .content .form-item1 .label{
  font-size: 32rpx;
  margin: 20rpx 0 20rpx 0;
  display: inline-block;
  color: rgba(0, 0, 0, 0.699);
}
.queren .content .form-item1 .value{
  font-size: 32rpx;
  margin: 20rpx 0 30rpx 0;
}
.queren .content .form-item2{
  height: 100rpx;
  /* background-color: rgb(145, 201, 138); */
  margin-left: 30rpx;
  display: flex;
}
.queren .content .form-item2 .label{
  font-size: 32rpx;
  margin: 20rpx 0 20rpx 0;
  display: inline-block;
  color: rgba(0, 0, 0, 0.699);
}
.queren .content .form-item2 .form-item2-border{
  height: 70rpx;
  width: 350rpx;
  border: 2rpx solid rgb(71, 174, 214);
  border-radius: 20rpx;
  line-height: 70rpx;
  margin-top: 5rpx;
}
.queren .content .form-item2 .form-item2-border text{
  margin-left: 20rpx;
  font-size: 35rpx;
}
.queren .content .form-item2 input{
  font-size: 35rpx;
  padding: 11rpx;
}
/* 预约须知样式 */
.xuzhi .title{
  font-size: 36rpx;
  margin: 30rpx 0 10rpx 30rpx;
}
.xuzhi .text{
  font-size: 24rpx;
  color: rgba(0, 0, 0, 0.699);
  margin-left: 30rpx;
}
/* 提交按钮 */
.click{
  margin-top: 30rpx;
  height: 160rpx;
}
.click button{
  background-color: rgb(6, 151, 170);
  color: rgb(255, 255, 255);
  border-radius: 50rpx;
}
